			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"firstName\");\r",
							"    pm.expect(pm.response.text()).to.include(\"lastName\");\r",
							"    pm.expect(pm.response.text()).to.include(\"countryCode\");\r",
							"    pm.expect(pm.response.text()).to.include(\"phoneNumber\");\r",
							"    pm.expect(pm.response.text()).to.include(\"zipCode\");\r",
							"});\r",
							"\r",
							"pm.test(\"firstName : to not be empty\", function () {    \r",
							"    var jsonData = pm.response.json();  \r",
							"  pm.expect(jsonData.firstName).to.not.be.empty});\r",
							"\r",
							"pm.test(\"lastName : to not be empty\", function () {    \r",
							"    var jsonData = pm.response.json();  \r",
							"  pm.expect(jsonData.lastName).to.not.be.empty;}\r",
							");\r",
							"\r",
							"pm.test(\"countryCode : to not be empty\", function () {    \r",
							"    var jsonData = pm.response.json();  \r",
							"  pm.expect(jsonData.mobilePhoneNumber.countryCode).to.not.be.empty});\r",
							"\r",
							"pm.test(\"phoneNumber : to not be empty\", function () {    \r",
							"    var jsonData = pm.response.json();  \r",
							"  pm.expect(jsonData.mobilePhoneNumber.phoneNumber).to.not.be.empty;}\r",
							");\r",
							"\r",
							"pm.test(\"zipCode : to not be empty\", function () {    \r",
							"    var jsonData = pm.response.json();  \r",
							"  pm.expect(jsonData.residencyAddress.zipCode).to.not.be.empty});\r",
							"\r",
							""
						],
